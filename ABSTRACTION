#focuses on the important details hiding complex implememntation 

class Animal(ABC):
def make_sound(self):
    pass

class Dog(Animal):
    def make_sound(self):
        return "Bark"
    
class Cat(Animal):
    def make_sound(self):
        return "meow" 
#using class
   dog=Dog()
   cat=Cat()    
print(dog.make_sound())#output:Bark

print(cat.make_sound())#output:Meow